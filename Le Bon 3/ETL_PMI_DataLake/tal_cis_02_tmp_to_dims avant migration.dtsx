              SQLTask:SqlStatementSource="-- v1 : product_type ne fait plus partie de la dimension D_PRODUCT , desactivation de l'alimentation de ce champs&#xA;merge [dbo].[D_PRODUCT] as P&#xA;using view_tmp_D_PRODUcT as V&#xA;on (P.brand_Subfamily = v.brand_Subfamily /*and P.brand_family = v.brand_family */ and P.origin = v.origin)&#xA;when matched then &#xA;update set P.product_code = v.product_code&#xA;,P.fa_code=v.fa_code&#xA;,P.brand_family=v.brand_family&#xA;,P.product_type = v.product_type&#xA;,P.trade_market_owner=v.trade_market_owner&#xA;,P.is_PMI_Brand = v.is_PMI_Brand&#xA;,P.EBROM_description=v.EBROM_description&#xA;&#xA;when not matched    then &#xA;insert ([product_code]&#xA;           ,[fa_code]&#xA;           ,[brand_subfamily]&#xA;           ,[brand_family]&#xA;          -- ,[product_type]&#xA;           ,[trade_market_owner]&#xA;           ,[EBROM_description]&#xA;           ,[is_PMI_brand]&#xA;           ,[origin]&#xA;           ,[updated_at])&#xA;&#x9;&#x9;   values&#xA;&#x9;&#x9;   (&#xA;&#x9;&#x9;&#x9;V.[product_code]&#xA;           ,v.[fa_code]&#xA;           ,v.[brand_subfamily]&#xA;           ,v.[brand_family]&#xA;          -- ,v.[product_type]&#xA;           ,v.[trade_market_owner]&#xA;           ,v.[EBROM_description]&#xA;           ,v.[is_PMI_brand]&#xA;           ,v.[origin]&#xA;           ,getdate()&#xA;&#x9;&#x9;   );&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
