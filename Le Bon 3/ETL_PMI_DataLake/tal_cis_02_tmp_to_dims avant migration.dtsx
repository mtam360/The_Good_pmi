              SQLTask:SqlStatementSource="-- D_city&#xA;insert into D_CITY (id_market,id_region,id_area, city , origin)&#xA;&#x9;select * from &#xA;&#x9;&#x9;&#x9;&#x9;(select &#xA;&#x9;&#x9;&#x9;&#x9;distinct &#xA;&#x9;&#x9;&#x9;&#x9;M.id_market, R.id_region,A.id_area, G.city, G.origin &#xA;&#x9;&#x9;&#x9;&#x9;from D_Geotree G inner join D_Market M on G.Market=M.Market&#xA;&#x9;&#x9;&#x9;&#x9;inner join D_REGION R on M.id_market = R.id_market and G.region = R.Region and G.origin = R.origin&#xA;&#x9;&#x9;&#x9;&#x9;inner join D_area A on M.id_market = A.id_market and R.id_region = A.id_region and G.area = A.area and G.origin = A.origin) &#xA;&#x9;&#x9;&#x9;&#x9;tmp &#xA;&#x9;&#x9;&#x9;&#x9;where not exists (select id_market,id_region, id_area,city,  origin from D_CITY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;where id_market = tmp.id_market and id_region = tmp.id_Region and id_area=tmp.id_area and city = tmp.City and  origin = tmp.origin )&#xA;&#x9;&#x9;&#x9;&#x9; &#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
