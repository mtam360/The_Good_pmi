              SQLTask:SqlStatementSource="USE [PAESNCIQA01]&#xA;GO&#xA;&#xA;truncate table [dbo].[D_GEOTREE];&#xA;INSERT INTO [dbo].[D_GEOTREE]&#xA;           ([Market]&#xA;           ,[Cluster]&#xA;           ,[Latitude]&#xA;           ,[Longitude]&#xA;           ,[Region]&#xA;           ,[Area]&#xA;           ,[Territory]&#xA;           ,[City]&#xA;           ,[origin]&#xA;           ,[updated_at])&#xA; -- TMP_RETAILERS&#xA; select &#xA;union_.country, cluster, latitude, Longitude, region, area, territory, city, origin, getdate()&#xA;from &#xA;(&#xA;select distinct  &#xA;upper(case when country='' OR country IS NULL THEN 'Unknown' ELSE COUNTRY end ) as country&#xA;,case when retailer_region='' OR retailer_region IS NULL THEN 'Unknown' ELSE retailer_region end as region&#xA; ,case when retailer_area='' OR retailer_area IS NULL THEN 'Unknown' ELSE retailer_area end as area&#xA; ,case when retailer_territory='' OR retailer_territory IS NULL THEN 'Unknown' ELSE retailer_territory end as territory&#xA;, 'Unknown' as city &#xA;,'Talos' as origin&#xA;from TMP_RETAILERS_TRANSACTIONS&#xA;union&#xA;-- TMP_CIS&#xA;select distinct * from &#xA;(&#xA;select distinct  &#xA;upper(case when Region='' OR Region IS NULL THEN 'Unknown' ELSE Region end)  as country&#xA;,case when Area='' OR Area IS NULL THEN 'Unknown' ELSE Area end as region&#xA; ,'Unknown' area&#xA; ,'Unknown' territory&#xA;, 'Unknown' as city &#xA;,'CIS' as origin&#xA;from TMP_CUSTOMERS_CONTACT_ONLINE&#xA;union &#xA;select distinct  &#xA;upper(case when Region='' OR Region IS NULL THEN 'Unknown' ELSE Region end) as country&#xA;,case when Area='' OR Area IS NULL THEN 'Unknown' ELSE Area end as region&#xA; ,'Unknown' area&#xA; ,'Unknown' territory&#xA;, 'Unknown' as city &#xA;,'CIS' as origin&#xA;from [dbo].[TMP_CUST_QUEST_ONLINE]&#xA;) d&#xA;) Union_&#xA;left join D_CLUSTER C on upper(union_.country) = upper (C.country)&#xA;GO&#xA;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
