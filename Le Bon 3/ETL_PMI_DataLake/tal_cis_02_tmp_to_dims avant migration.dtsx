            <SQLTask:SqlTaskData
              SQLTask:Connection="{043D9592-D2E6-4C74-99E3-BDE4EAE39457}"
              SQLTask:SqlStatementSource="-- D_Territory&#xA;insert into D_TERRITORY(id_market,id_region,id_area, id_city , Territory, origin)&#xA;&#x9;select * from &#xA;&#x9;&#x9;&#x9;&#x9;(select &#xA;&#x9;&#x9;&#x9;&#x9;distinct &#xA;&#x9;&#x9;&#x9;&#x9;M.id_market, R.id_region ,A.id_area, C.id_city, G.Territory, G.origin &#xA;&#x9;&#x9;&#x9;&#x9;from D_Geotree G inner join D_Market M on G.Market=M.Market&#xA;&#x9;&#x9;&#x9;&#x9;inner join D_REGION R on M.id_market = R.id_market and G.region = R.Region and G.origin = R.origin&#xA;&#x9;&#x9;&#x9;&#x9;inner join D_area A on M.id_market = A.id_market and R.id_region = A.id_region and G.area = A.area and G.origin = A.origin&#xA;&#x9;&#x9;&#x9;&#x9;inner join D_City C on M.id_market = C.id_market and R.id_region = C.id_region and A.id_area = C.id_area and G.city = C.city and G.origin = C.origin) &#xA;&#x9;&#x9;&#x9;&#x9;tmp &#xA;&#x9;&#x9;&#x9;&#x9;where not exists (select id_market,id_region, id_area,id_city, territory, origin from D_TERRITORY&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;where id_market = tmp.id_market and id_region = tmp.id_Region and id_area=tmp.id_area and id_city = tmp.id_City and territory = tmp.Territory and  origin = tmp.origin )&#xA;&#x9;&#x9;&#x9;&#x9; " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
